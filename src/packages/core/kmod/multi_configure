CC="gcc -m32" ./configure \
    --prefix=/usr                 \
    --bindir=/bin                 \
    --libdir=/usr/lib32          \
    --sysconfdir=/etc             \
    --with-xz                     \
    --with-zlib                   \
    --host=i686-pc-linux-gnu      \
    --with-rootlibdir=/usr/lib32