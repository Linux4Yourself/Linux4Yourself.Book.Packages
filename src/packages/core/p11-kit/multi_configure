CC="gcc -m32" CXX="gcc -m32" PKG_CONFIG_PATH="/usr/lib32/pkgconfig"  ./configure \
    --prefix=/usr                       \
    --sysconfdir=/etc                   \
    --with-trust-paths=/etc/pki/anchors \
    --libdir=/usr/lib32                 \
    --host=i686-pc-linux-gnu            \
    --without-systemd 
